{
  "name": "vue-tags-input",
  "description": "Vue Tags Input ",
  "version": "1.0.0",
  "author": "Johannes Munari <johannesm@hotmail.de>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --open --hot",
    "test-sw": "hs -c 0 -p 8111 ./dist & cross-env NODE_ENV=production webpack -w",
    "build": "cross-env webpack --hide-modules",
    "analyze-build": "cross-env NODE_ENV=production ANALYZE=true webpack",
    "precommit": "lint-staged",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint-fix": "eslint --fix --ignore-path .gitignore .",
    "e2e": "testcafe all e2e/*test* --app \"npm run dev\" --app-init-delay 10000 -S -s screenshots"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.vue": "eslint"
  },
  "dependencies": {
    "vue": "^2.5.11"
  },
  "devDependencies": {
    "@hydrant/eslint-config": "^1.1.2",
    "autoprefixer": "^7.2.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.1",
    "eslint-plugin-html": "^3.2.1",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "image-webpack-loader": "^3.4.2",
    "js-beautify": "1.6.14",
    "node-sass": "^4.7.2",
    "normalize-css": "^2.3.1",
    "postcss-loader": "^2.0.9",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "testcafe": "^0.18.5",
    "testcafe-vue-selectors": "^2.1.0",
    "vue-loader": "^13.6.0",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.11",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.7"
  },
  "eslintConfig": {
    "extends": "@hydrant/eslint-config/vue"
  }
}
